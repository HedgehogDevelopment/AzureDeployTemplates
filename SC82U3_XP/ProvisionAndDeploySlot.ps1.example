#Import the stuff we need 
Import-Module AzureRM

#Point to the sitecore cloud tools on your local filesystem
Import-Module c:\temp\SitecoreAzure\Tools\Sitecore.Cloud.Cmdlets.psm1

#Add the azure account
Add-AzureRmAccount

$Name="[deployment name]"

Start-SitecoreAzureDeployment -Location "East US" -Name $Name -ArmTemplateUrl "https://????.blob.core.windows.net/sitecore/xp/azuredeploy.cd_slot.json" -ArmParametersPath ".\azuredeploy.parameters_slot.json" -LicenseXmlPath "????\license.xml" -Verbose
